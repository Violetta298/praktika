<header>
    <nav class="nav-extended">
        <div class="nav-wrapper">
          <a href="/main" class="brand-logo">Админка</a>
        </div>
        <div class="nav-content">
          <ul class="tabs tabs-transparent">
            <li class="tab"><a class="active" href="#block">Заблокировать</a></li>
            <li class="tab"><a  href="#unblock">Разблокировать</a></li>
            <li class="tab"><a href="#setapi">Изменить статус API</a></li>
            <% if(user.role === 'SUPER_ADMIN') {  %>
                <li class="tab"><a href="#remove">Забрать статус</a></li>
                <li class="tab"><a href="#give">Дать статус</a></li>
            <% } %>
          </ul>
        </div>
      </nav>
    
    
      <div id="block" class="col s12 container">
            <% notBlocked.forEach(item => { %>
                <div class="row">
                    <div class="col s12 m12">
                      <div class="card blue-grey darken-1">
                        <div class="card-content white-text">
                          <span class="card-title">Пользователь</span>
                          <p>Логин: <%- item.login -%></p>
                          <p>Email: <%- item.email -%></p>
                          <p>API: <%- item.api -%></p>
                          <p>Роль: <%- item.role -%></p>
                        </div>
                        <div class="card-action">
                          <a class="block" data-id='<%- item._id -%>' href="#">Заблокировать</a>
                        </div>
                      </div>
                    </div>
                  </div>
            <% }) %>
      </div>
      <div id="unblock" class="col s12 container">
        <% blocked.forEach(item => { %>
            <div class="row">
                <div class="col s12 m12">
                  <div class="card blue-grey darken-1">
                    <div class="card-content white-text">
                      <span class="card-title">Пользователь</span>
                      <p>Логин: <%- item.login -%></p>
                      <p>Email: <%- item.email -%></p>
                      <p>API: <%- item.api -%></p>
                      <p>Роль: <%- item.role -%></p>
                    </div>
                    <div class="card-action">
                      <a class="unblock" data-id='<%- item._id -%>' href="#">Разблокировать</a>
                    </div>
                  </div>
                </div>
            </div>
        <% }) %>
      </div>
      <div id="setapi" class="col s12 container">
        <% users.forEach(item => { %>
            <div class="row ">
                <div class="col s12 m12">
                  <div class="card blue-grey darken-1">
                    <div class="card-content white-text">
                      <span class="card-title">Пользователь</span>
                      <p>Логин: <%- item.login -%></p>
                      <p>Email: <%- item.email -%></p>
                      <p>API: <%- item.api -%></p>
                      <p>Роль: <%- item.role -%></p>
                    </div>
                    <div class="card-action">
                      <a class="setapi" data-id='<%- item._id -%>' href="#">Изменить статус API</a>
                    </div>
                  </div>
                </div>
              </div>
        <% }) %>
      </div>

      <% if(user.role === 'SUPER_ADMIN') {  %>
        <div id="give" class="col s12 container">
            <% notAdmins.forEach(item => { %>
                <div class="row ">
                    <div class="col s12 m12">
                      <div class="card blue-grey darken-1">
                        <div class="card-content white-text">
                          <span class="card-title">Пользователь</span>
                          <p>Логин: <%- item.login -%></p>
                          <p>Email: <%- item.email -%></p>
                          <p>API: <%- item.api -%></p>
                          <p>Роль: <%- item.role -%></p>
                        </div>
                        <div class="card-action">
                          <a class="give" data-id='<%- item._id -%>' href="#">Выдать админку</a>
                        </div>
                      </div>
                    </div>
                  </div>
            <% }) %>
          </div>
    
          <div id="remove" class="col s12 container">
            <% admins.forEach(item => { %>
                <div class="row ">
                    <div class="col s12 m12">
                      <div class="card blue-grey darken-1">
                        <div class="card-content white-text">
                          <span class="card-title">Пользователь</span>
                          <p>Логин: <%- item.login -%></p>
                          <p>Email: <%- item.email -%></p>
                          <p>API: <%- item.api -%></p>
                          <p>Роль: <%- item.role -%></p>
                        </div>
                        <div class="card-action">
                          <a class="remove" data-id='<%- item._id -%>' href="#">Забрать админку</a>
                        </div>
                      </div>
                    </div>
                  </div>
            <% }) %>
          </div>
    <% } %>

</header>